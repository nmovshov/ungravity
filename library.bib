issn = {00319201},
journal = {Phys. Earth Planet. Inter.},
keywords = {Maclaurin spheroid,Planetary interiors,Planetary shape,Rotational flattening of planets,Theory of figures,gravity,jupiter},
mendeley-tags = {gravity,jupiter},
number = {3-4},
pages = {364--379},
publisher = {Elsevier B.V.},
