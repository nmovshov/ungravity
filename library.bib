journal = {Icarus},
keywords = {Accretion,Asteroids,Origin,Planetesimals,Solar System},
number = {2},
pages = {671--684},
publisher = {Elsevier Inc.},
title = {{Initial sizes of planetesimals and accretion of the asteroids}},
volume = {214},
year = {2011}
}
