@misc{Nelson2018,
abstract = {We present results from a data challenge posed to the radial velocity (RV) community: namely, to quantify the "evidence" for {\$}n=\backslash{\{}0, 1, 2, 3\backslash{\}}{\$} planets in a set of synthetically generated RV datasets containing a range of planet signals. Participating teams were provided the same likelihood function and set of priors to use in their analysis. They applied a variety of methods to estimate {\$}\backslashwidehat{\{}\backslashmathcal{\{}Z{\}}{\}}{\$}, the marginal likelihood for each {\$}n{\$}-planet model, including cross-validation, the Laplace approximation, importance sampling, and nested sampling. We found the dispersion in {\$}\backslashwidehat{\{}\backslashmathcal{\{}Z{\}}{\}}{\$} across different methods grew with increasing {\$}n{\$}-planet models: {\$}\backslashsim\backslash,{\$}3 for 0-planets, {\$}\backslashsim\backslash,{\$}10 for 1-planet, {\$}\backslashsim\backslash,{\$}{\$}10{\^{}}2{\$}-{\$}10{\^{}}3{\$} for 2-planets, and {\$}{\textgreater}\backslash,{\$}{\$}10{\^{}}4{\$} for 3-planets. Most internal estimates of uncertainty in {\$}\backslashwidehat{\{}\backslashmathcal{\{}Z{\}}{\}}{\$} for individual methods significantly underestimated the observed dispersion across all methods. Methods that adopted a Monte Carlo approach by comparing estimates from multiple runs yielded plausible uncertainties. Finally, two classes of numerical algorithms (those based on importance and nested samplers) arrived at similar conclusions regarding the ratio of {\$}\backslashwidehat{\{}\backslashmathcal{\{}Z{\}}{\}}{\$}s for {\$}n{\$} and ({\$}n+1{\$})-planet models. One analytic method (the Laplace approximation) demonstrated comparable performance. We express both optimism and caution: we demonstrate that it is practical to perform rigorous Bayesian model comparison for {\$}\backslashleq{\$}3-planet models, yet robust planet discoveries require researchers to better understand the uncertainty in {\$}\backslashwidehat{\{}\backslashmathcal{\{}Z{\}}{\}}{\$} and its connections to model selection. Future research should address greater {\$}n{\$}-planet models, RVs with stellar activity indicators, and many more simulated datasets with a wider range of planet and noise properties.},
archivePrefix = {arXiv},
arxivId = {1806.04683},
author = {Nelson, Benjamin E. and Ford, Eric B. and Buchner, Johannes and Cloutier, Ryan and D{\'{i}}az, Rodrigo F. and Faria, Jo{\~{a}}o P. and Rajpaul, Vinesh M. and Rukdee, Surangkhana},
eprint = {1806.04683},
file = {:C$\backslash$:/Users/nmovs/Documents/Mendeley Desktop/2018/Nelson et al., 2018.pdf:pdf},
isbn = {ISSN-0898-5952},
keywords = {activity,analytical,data analysis,detection,methods,numerical,planets and satellites,radial velocities,stars,statistical,techniques},
title = {{Quantifying the Evidence for a Planet in Radial Velocity Data}},
year = {2018}
}
