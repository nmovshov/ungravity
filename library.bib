file = {:C$\backslash$:/Users/nmovs/Documents/Mendeley Desktop/2009/Marcus et al., 2009.pdf:pdf},
isbn = {2041-8205},
issn = {0004-637X},
keywords = {formation,planetary systems,planets and satellites},
pages = {L118--L122},
title = {{Collisional Stripping and Disruption of Super-Earths}},
volume = {700},
year = {2009}
}
