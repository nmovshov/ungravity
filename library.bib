journal = {Icarus},
keywords = {Collisional physics,Cratering,Impact processes,Moon,SPA},
mendeley-tags = {SPA},
number = {2},
pages = {730--743},
publisher = {Elsevier Inc.},
title = {{Constraining the size of the South Pole-Aitken basin impact}},
volume = {220},
year = {2012}
